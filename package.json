{
  "name": "be-assessment",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon",
    "build": "tsc && tsc-alias",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "sh -c 'npm run typeorm migration:generate -o -p src/database/migrations/$0 -- -d src/database/data-source.ts'",
    "migration:run": "npm run typeorm migration:run -- -d src/database/data-source.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/database/data-source.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "http-status": "^2.1.0",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.1",
    "@types/swagger-ui-express": "^4.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
